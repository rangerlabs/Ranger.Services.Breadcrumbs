{
    "cloudSql": {
        "ConnectionString": "Host=breadcrumbs_postgres;Port=5432;Username=postgres;Password=postgres;Database=breadcrumbs"
    },
    "DataProtectionCertPath": {
        "Path": "./Certs/data-protection-cert.pfx"
    },
    "rabbitMQ": {
        "host": "rabbitmq",
        "port": "5672",
        "virtualHost": "/",
        "username": "breadcrumbsUser",
        "password": "breadcrumbsPass",
        "namespace": "breadcrumbs",
        "retries": 3,
        "retryInterval": 3
    },
    "httpClient": {
        "identityAuthority": "http://localhost.io:5000"
    },
    "Serilog": {
        "Using": ["Serilog.Sinks.Console"],
        "MinimumLevel": {
            "Default": "Debug",
            "Override": {
                "Microsoft": "Information",
                "System": "Warning"
            }
        },
        "Enrich": ["FromLogContext"],
        "WriteTo": [
            {
                "Name": "DatadogLogs",
                "Args": {
                    "apiKey": "",
                    "service": "Breadcrumbs",
                    "source": "Serilog",
                    "host": "Nick's Laptop"
                }
            }
        ]
    }
}
